import java.util.Arrays;

public class StringPool {

	enum abc {
		max, min;
	}

	public static void main(String[] args) {

		int[] a = { 2, 1, 6, 4, 3 };
		Arrays.sort(a);
		int n = a[a.length - 1];
		int sum = n * (n + 1) / 2;
		int m = 0;

		for (int i = 0; i < a.length; i++) {
			m = m + a[i];
		}
		System.out.println(sum - m);
	}

}
