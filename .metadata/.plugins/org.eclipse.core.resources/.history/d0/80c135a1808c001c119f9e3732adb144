
public class StringPool {

	enum abc {
		max, min;
	}

	public static void main(String[] args) {

		int[] a = { 1, 2, 3, 5, 6 };
		/*
		 * while (i <= a.length) { if (i != a[i - 1]) { arr[m] = i; m++; } i++; } for
		 * (int j = 0; j < arr.length; j++) { if (arr[j] != 0) {
		 * System.out.println(arr[j]); } }
		 */
		int n = a.length;
		System.out.println((n * (n + 1)) / 2);
	}

}
