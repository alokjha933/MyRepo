
public class StringPool {

	enum abc {
		max, min;
	}

	public static void main(String[] args) {

		int[] a = { 1, 3, 4, 5, 6 };
		int s = 0;
		for (int i = 0; i < a.length; i++) {
			s += a[i];
		}
		if (s % 2 != 0) {
			System.out.println(1 + Math.abs(s / a.length));
		} else {
			System.out.println(Math.absExact(s / a.length));
		}
	}

}
